	SUBSP	2,i
	CALL	main
	ADDSP	2,i
	STOP
putint:	DECO	2,s
	LDWA	2,s
	STWA	4,s
	RET
getint:	DECI	2,s
	RET
skylersFun:	NOP0
	LDWA	7,i
	SUBSP	2,i
	STWA	0,s
	LDWA	7,i
	CPWA	0,i
	BREQ	__else2
	LDWA	7,i
	SUBSP	2,i
	STWA	0,s
	LDWA	7,i
	ADDA	0,s
	STWA	0,s
	ADDSP	2,i
	BR	__end2
__else2:	NOP0
	SUBSP	2,i
	LDWA	7,i
	SUBSP	2,i
	STWA	0,s
	CALL	putint
	ADDSP	4,i
	LDWA	-2,s
__end2:	NOP0
	STWA	2,s
	RET
main:	NOP0
	SUBSP	2,i
	CALL	skylersFun
	ADDSP	2,i
	LDWA	-2,s
	STWA	2,s
	RET
	.END
