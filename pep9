;Start Of Main Method
	SUBSP	2,i
	CALL	main
	ADDSP	2,i
	STOP

;putint will print out an integer value.
putint:	DECO	2,s
	LDWA	2,s
	STWA	4,s
	RET

;getint will retrieve input.
getint:	DECI	2,s
	RET

;getchar will retrieve input and store the ASCII equivelant.
getchar:	LDWA	0,i
	LDBA	charIn,d
	STWA	2,s
	RET

;putchar will print out an ASCII equivelant.
putchar:	LDWA	2,s
	STBA	charOut,d
	STWA	4,s
	RET

skyFun:	NOP0
	SUBSP	2,i
	LDWA	27,i
	SUBSP	2,i
	STWA	0,s
	LDWA	6,s
	ADDA	0,s
	STWA	0,s
	ADDSP	2,i
	SUBSP	2,i
	STWA	0,s
	CALL	putint
	ADDSP	4,i
	LDWA	-2,s
	STWA	4,s
	RET

main:	NOP0
	SUBSP	2,i
	LDWA	14,i
	SUBSP	2,i
	STWA	0,s
	CALL	skyFun
	ADDSP	4,i
	LDWA	-2,s
	STWA	2,s
	RET
	.END
