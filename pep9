	SUBSP	2,i
	CALL	main
	ADDSP	2,i
	STOP

putint:	DECO	2,s
	LDWA	2,s
	STWA	4,s
	RET

getint:	DECI	2,s
	RET

getchar:LDWA	0,i
	LDBA	charIn,d
	STWA	2,s
	RET
putchar:STBA	charOut,d
	RET
skyFun:	NOP0
	LDWA	7,i
	SUBSP	2,i
	STWA	0,s
	LDWA	7,i
	NEGA	
	ADDA	0,s
	STWA	0,s
	ADDSP	2,i
	CPWA	0,i
	BREQ	__else2
	SUBSP	2,i
	LDWA	6,s
	SUBSP	2,i
	STWA	0,s
	CALL	putint
	ADDSP	4,i
	LDWA	-2,s
	BR	__end2
__else2:	NOP0
	SUBSP	2,i
	LDWA	4,s
	SUBSP	2,i
	STWA	0,s
	CALL	putint
	ADDSP	4,i
	LDWA	-2,s
__end2:	NOP0
	LDWA	0,i
	STWA	2,s
	RET
main:	NOP0
	SUBSP	2,i
	LDWA	7,i
	SUBSP	2,i
	STWA	0,s
	LDWA	22,i
	SUBSP	2,i
	STWA	0,s
	LDWA	17,i
	SUBSP	2,i
	STWA	0,s
	CALL	skyFun
	ADDSP	8,i
	LDWA	-2,s
	LDWA	0,i
	STWA	2,s
	RET
	.END
