	SUBSP	2,i
	CALL	main
	ADDSP	2,i
	STOP

putint:	DECO	2,s
	LDWA	2,s
	STWA	4,s
	RET

getint:	DECI	2,s
	RET

getchar:LDWA	0,i
	LDBA	charIn,d
	STWA	2,s
	RET
putchar:STBA	charOut,d
	RET
skyFun:	NOP0
	SUBSP	2,i
	LDWA	1,i
	SUBSP	2,i
	STWA	0,s
	SUBSP	2,i
	STWA	0,s
	LDWA	8,s
	STWA	0,s
	SUBSP	2,i
	STWA	0,s
	LDWA	10,i
	NEGA	
	ADDA	0,s
	STWA	0,s
	ADDSP	2,i
	NEGA
	CALL	putint
	ADDSP	4,i
	LDWA	-2,s
	LDWA	0,i
	STWA	2,s
	RET
main:	NOP0
	SUBSP	2,i
	LDWA	7,i
	SUBSP	2,i
	STWA	0,s
	LDWA	22,i
	SUBSP	2,i
	STWA	0,s
	LDWA	17,i
	SUBSP	2,i
	STWA	0,s
	CALL	skyFun
	ADDSP	8,i
	LDWA	-2,s
	LDWA	0,i
	STWA	2,s
	RET
	.END
