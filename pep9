	SUBSP	2,i
	CALL	main
	ADDSP	2,i
	STOP
putint:	DECO	2,s
	LDWA	2,s
	STWA	4,s
	RET
getint:	DECI	2,s
	RET
main:	NOP0
	LDWA	1,i
	SUBSP	2,i
	STWA	0,s
	LDWA	9,i
	ADDA	0,s
	STWA	0,s
	SUBSP	2,i
	CALL	getint
	ADDSP	2,i
	ADDA	-2,s
	STWA	0,s
	LDWA	1,i
	SUBSP	2,i
	STWA	0,s
	LDWA	3,i
	NEGA	
	ADDA	0,s
	STWA	0,s
	ADDSP	2,i
	ADDA	0,s
	STWA	0,s
	ADDSP	2,i
	STWA	2,s
	RET
	.END
