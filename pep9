	SUBSP	2,i
	CALL	main
	ADDSP	2,i
	STOP

putint:	DECO	2,s
	LDWA	2,s
	STWA	4,s
	RET

getint:	DECI	2,s
	RET

getchar:LDWA	0,i
	LDBA	charIn,d
	STWA	2,s
	RET
putchar:LDWA	2,s
	STBA	charout,d
	STWA	4,s
	RET
main:	NOP0
	LDWA	1,i
	SUBSP	2,i
	STWA	0,s
	LDWA	1,i
	CPWA	0,i
	BREQ	__else2
	SUBSP	2,i
	LDWA	1,i
	SUBSP	2,i
	STWA	0,s
	CALL	putint
	ADDSP	4,i
	LDWA	-2,s
	BR	__end2
__else2:	NOP0
	SUBSP	2,i
	LDWA	3,i
	SUBSP	2,i
	STWA	0,s
	CALL	putint
	ADDSP	4,i
	LDWA	-2,s
__end2:	NOP0
	STWA	2,s
	RET
	.END
