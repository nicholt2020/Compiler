;Start Of Main Method
	SUBSP	2,i
	CALL	main
	ADDSP	2,i
	STOP

;putint will print out an integer value.
putint:	DECO	2,s
	LDWA	2,s
	STWA	4,s
	RET

;getint will retrieve input.
getint:	DECI	2,s
	RET

;getchar will retrieve input and store the ASCII equivelant.
getchar:	LDWA	0,i
	LDBA	charIn,d
	STWA	2,s
	RET

;putchar will print out an ASCII equivelant.
putchar:	LDWA	2,s
	STBA	charOut,d
	STWA	4,s
	RET
;myfun_x 4
;myfun_y 2

myfun:	NOP0
	LDWA	2,s
	STWA	4,s
	SUBSP	2,i
	CALL	getint
	ADDSP	2,i
	LDWA	-2,s
	STWA	2,s
	LDWA	2,s
	STWA	4,s
	SUBSP	2,i
	STWA	0,s
	LDWA	2,i
	NEGA	
	ADDA	0,s
	STWA	0,s
	ADDSP	2,i
	CPWA	0,i
	BREQ	__else2
	LDWA	4,s
	STWA	4,s
	LDWA	10,i
	SUBSP	2,i
	STWA	0,s
	LDWA	20,i
	NEGA
	ADDA	0,s
	STWA	0,s
	LDWA	4,s
	STWA	4,s
	ADDA	0,s
	STWA	0,s
	ADDSP	2,i
	STWA	4,s
	LDWA	2,s
	STWA	4,s
	LDWA	3,i
	STWA	2,s
	BR	__end2
__else2:	NOP0
	LDWA	2,s
	STWA	4,s
	LDWA	25,i
	STWA	2,s
__end2:	NOP0
	SUBSP	2,i
	LDWA	6,s
	STWA	4,s
	SUBSP	2,i
	STWA	0,s
	CALL	putint
	ADDSP	4,i
	LDWA	-2,s
	SUBSP	2,i
	LDWA	32,i
	SUBSP	2,i
	STWA	0,s
	CALL	putchar
	ADDSP	4,i
	LDWA	-2,s
	SUBSP	2,i
	LDWA	4,s
	STWA	4,s
	SUBSP	2,i
	STWA	0,s
	CALL	putint
	ADDSP	4,i
	LDWA	-2,s
	SUBSP	2,i
	LDWA	10,i
	SUBSP	2,i
	STWA	0,s
	CALL	putchar
	ADDSP	4,i
	LDWA	-2,s
	LDWA	0,i
	STWA	2,s
	RET

main:	NOP0
	SUBSP	2,i
	LDWA	0,i
	SUBSP	2,i
	STWA	0,s
	LDWA	0,i
	SUBSP	2,i
	STWA	0,s
	CALL	myfun
	ADDSP	6,i
	LDWA	-2,s
	STWA	2,s
	RET
	.END
