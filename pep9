	SUBSP	2,i
	CALL	main
	ADDSP	2,i
	STOP
putint:	DECO	2,s
	LDWA	2,s
	STWA	4,s
	RET
getint:	DECI	2,s
	RET
main:	NOP0
	LDWA	51,i
	SUBSP	2,i
	STWA	0,s
	LDWA	51,i
	NEGA	
	ADDA	0,s
	STWA	0,s
	ADDSP	2,i
	CPWA	0,i
	BREQ	__else2
	LDWA	2,i
	BR	__end2
__else2:	NOP0
	LDWA	3,i
	CPWA	0,i
	BREQ	__else3
	LDWA	22,i
	BR	__end3
__else3:	NOP0
	LDWA	1,i
__end3:	NOP0
__end2:	NOP0
	STWA	2,s
	RET
	.END
