	SUBSP	2,i
	CALL	main
	ADDSP	2,i
	STOP


putint:	DECO	2,s
	LDWA	2,s
	STWA	4,s
	RET


getint:	DECI	2,s
	RET


x	.EQUATE 0
y	.EQUATE 2
z	.EQUATE 4
skyFun:	NOP0
	SUBSP	2,i
	LDWA	3,i
	SUBSP	2,i
	STWA	0,s
	CALL	putint
	ADDSP	4,i
	LDWA	-2,s
	SUBSP	2,i
	LDWA	2,i
	SUBSP	2,i
	STWA	0,s
	CALL	putint
	ADDSP	4,i
	LDWA	-2,s
	LDWA	0,i
	STWA	2,s
	RET


main:	NOP0
	SUBSP	2,i
	SUBSP	2,i
	CALL	skyFun
	ADDSP	8,i
	LDWA	-2,s
	SUBSP	2,i
	STWA	0,s
	CALL	putint
	ADDSP	4,i
	LDWA	-2,s
	STWA	2,s
	RET
	.END
