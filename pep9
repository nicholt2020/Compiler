s:	.EQUATE 0

	SUBSP	4,i
	CALL	main
	ADDSP	4,i
	STOP
putint:	DECO	2,s
	LDWA	2,s
	STWA	4,s
	RET
getint:	DECI	2,s
	RET
main:	NOP0
	LDWA	8,i
	STWA	 s,x
	LDWA	s,x
	SUBA	1,i
	STWA	s,x
	LDWA	s,x
	ADDA	1,i
	STWA	s,x
	LDWA	s,x
	ADDA	8,i
	STWA	s,x
	SUBSP	2,i
	CALL	getint
	ADDSP	2,i
	LDWA	s,x
	ADDA	-2,s
	STWA	s,x
	STWA	2,s
	RET
	.END
